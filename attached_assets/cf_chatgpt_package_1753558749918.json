{
  "name": "chitty-chatgpt-mcp",
  "version": "1.0.0",
  "description": "Cloudflare MCP server for ChatGPT integration with ChittyOS",
  "main": "src/index.ts",
  "scripts": {
    "build": "wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:prod": "wrangler deploy --env production",
    "dev": "wrangler dev",
    "dev:local": "tsx src/index.ts",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "chatgpt",
    "openai",
    "cloudflare",
    "chittyos",
    "workers"
  ],
  "author": "ChittyOS",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/node": "^20.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^3.78.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}